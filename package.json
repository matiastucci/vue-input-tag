{
  "name": "vue-input-tag",
  "version": "1.0.4",
  "description": "Vue.js input tag editor component",
  "keywords": [
    "input",
    "tag",
    "inputtag"
  ],
  "author": "Matias Tucci <matiastucci@gmail.com>",
