    "vue-server-renderer": "^2.4.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.4.5"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 4.0.0"
  },
  "jest": {
